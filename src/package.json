{
  "name": "polish-api-aspsp-mock",
  "version": "1.0.0",
  "description": "Mock implementation of ASPSP exposing Polish API interface.",
  "main": "target/index.js",
  "scripts": {
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start": "node target",
    "generate-data": "node target/generator"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Grzegorz Abramczyk <grzegorz.abramczyk@tuatara.pl>",
  "license": "MIT",
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/chai": "^4.2.11",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/js-yaml": "^3.12.3",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/session-file-store": "^1.2.1",
    "@types/swagger-tools": "^0.10.6",
    "@types/uuid": "^7.0.3",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.0",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jws": "^4.0.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "polish-api-generator": "git://github.com/tuatarapl/polish-api-generator.git",
    "polish-api-model": "git://github.com/tuatarapl/polish-api-model.git",
    "session-file-store": "^1.4.0",
    "swagger-tools": "^0.10.4",
    "uuid": "^8.0.0"
  }
}
